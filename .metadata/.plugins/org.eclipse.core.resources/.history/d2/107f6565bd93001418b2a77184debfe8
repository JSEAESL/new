	{
		public static var ins:KeyView = new KeyView()
		public function KeyView()
		{
			init();
		}
		
		private var showText:TextField;
		private function init():void
		{
			showText = new TextField();
			addChild(showText);
			showText.width = 
			showText.text = "THIS IS KEY VIEW";
		}
	}
}